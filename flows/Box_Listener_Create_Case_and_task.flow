    <description>Creates box case when a client uploads a file in box</description>
    <formulas>
        <name>Description</name>
        <dataType>String</dataType>
        <expression>TEXT({!$Flow.CurrentDate})+&apos;- &apos;+{!Filename} +&apos;  Uploaded to &apos;+ {!FolderName} +&apos;  folder&apos;</expression>
    </formulas>
    <formulas>
        <name>Subject</name>
        <dataType>String</dataType>
        <expression>&quot;File Uploaded in Client&apos;s  &quot; +if({!BaseFoldername}!=&apos;&apos;, {!BaseFoldername},&apos;Other&apos;) +&quot; Folder&quot;</expression>
    </formulas>
    <formulas>
        <name>Wait</name>
        <dataType>DateTime</dataType>
        <expression>NOW() - (1/26)</expression>
    </formulas>
