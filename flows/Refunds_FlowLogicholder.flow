    <decisions>
        <name>Refund_to_credit</name>
        <label>Refund to credit</label>
        <locationX>666</locationX>
        <locationY>1223</locationY>
        <defaultConnector>
            <targetReference>Research_Complete</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>False</defaultConnectorLabel>
        <rules>
            <name>Type_From_Refund_to_Credit</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Request_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Credit</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Request_Type__c</leftValueReference>
