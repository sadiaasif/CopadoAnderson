    <description>Added Active User condition in &quot;Get User&quot; Element</description>
    <formulas>
        <name>Needs_Update</name>
        <dataType>Boolean</dataType>
        <expression>OR(
({!Loop_Order_Item.Active_User__r.FirstName} + &apos; &apos; + {!Loop_Order_Item.Active_User__r.LastName}) = {!Previous_Law_Coordinator},
ISBLANK({!Previous_Law_Coordinator})
)</expression>
    </formulas>
    <formulas>
        <name>OSI_Completed</name>
        <dataType>Boolean</dataType>
        <expression>OR(
ISPICKVAL ( {!Loop_Order_Item.Processing_Stage__c} ,  &quot;Completed&quot; ),
ISPICKVAL ( {!Loop_Order_Item.Processing_Stage__c} ,  &quot;Kitted&quot; ),
ISPICKVAL ( {!Loop_Order_Item.Processing_Stage__c} ,  &quot;BCP Scheduled&quot; ),
ISPICKVAL ( {!Loop_Order_Item.Processing_Stage__c} ,  &quot;Exchanged for Other Services&quot; ),
ISPICKVAL ( {!Loop_Order_Item.Processing_Stage__c} ,  &quot;Complete, OG Dissolution started&quot; ),
ISPICKVAL ( {!Loop_Order_Item.Processing_Stage__c} ,  &quot;Welcome Call Completed&quot; ),
ISPICKVAL ( {!Loop_Order_Item.Processing_Stage__c} ,  &quot;Client unresponsive&quot; ),
ISPICKVAL ( {!Loop_Order_Item.Processing_Stage__c} ,  &quot;Credit Issued&quot; ),
ISPICKVAL ( {!Loop_Order_Item.Processing_Stage__c} ,  &quot;Future Use&quot; )
)</expression>
    </formulas>
