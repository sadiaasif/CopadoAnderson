    <decisions>
        <description>Does this opportunity have one of the relevant services for Unlimited Land Trusts</description>
        <name>ULT_Service</name>
        <label>ULT Service?</label>
        <locationX>168</locationX>
        <locationY>346</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ServiceName</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Account_Information</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>Determines if the client has unlimited land trust.</description>
    <formulas>
        <name>BackInTime55Minutes</name>
        <dataType>DateTime</dataType>
        <expression>NOW() - (1/26)</expression>
    </formulas>
    <formulas>
        <description>Date Unlimited Land Trust Service Ends</description>
        <name>ULTEndDate</name>
        <dataType>String</dataType>
        <expression>TEXT(DATE(YEAR({!ApprovedTxDate})+7, MONTH({!ApprovedTxDate}), DAY({!ApprovedTxDate})))</expression>
    </formulas>
    <interviewLabel>Unlimited Land Trust {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Unlimited Land Trust</label>
