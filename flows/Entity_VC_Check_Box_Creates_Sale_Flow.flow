        <defaultConnectorLabel>End</defaultConnectorLabel>
        <rules>
            <name>GreenHQ_is_True</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>GreenHQ</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_HQ_Sale</targetReference>
            </connector>
            <label>GreenHQ is True</label>
        </rules>
    </decisions>
    <formulas>
        <name>CloseDate</name>
        <dataType>Date</dataType>
        <expression>DATE(
YEAR(TODAY()) + 1,
IF(MONTH(TODAY()) = 12, 1, MONTH(TODAY()) + 1),
1) - 1</expression>
    </formulas>
    <formulas>
        <name>GreenHQ</name>
        <dataType>Boolean</dataType>
        <expression>IF(
AND( 
NOT(ISBLANK({!GreenCorpHQDate})), 
TODAY() &lt; ({!GreenCorpHQDate} + 730), 
ISBLANK({!NexusCurrent})),
TRUE,
FALSE)</expression>
    </formulas>
    <formulas>
        <name>RenewalEffDate</name>
        <dataType>Date</dataType>
        <expression>{!CloseDate} + 1</expression>
    </formulas>
    <formulas>
        <name>RenewalEndDate</name>
        <dataType>Date</dataType>
        <expression>DATE(
YEAR({!CloseDate}) + 1,
MONTH({!CloseDate}),
DAY({!CloseDate}))</expression>
    </formulas>
