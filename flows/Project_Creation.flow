    <decisions>
        <description>Checks to see if the product is one that needs a project created. Add new products here.</description>
        <name>Project_Needed</name>
        <label>Project Needed?</label>
        <locationX>689</locationX>
        <locationY>369</locationY>
        <defaultConnectorLabel>No Project</defaultConnectorLabel>
        <rules>
            <name>Yes_Project</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>loopVarServices.Product2Id__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>01tU0000001neux</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loopVarServices.Product2Id__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>01tU0000001neun</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loopVarServices.Product2Id__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>01tU0000001nevb</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loopVarServices.Product2Id__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>01tU0000001q3yZ</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loopVarServices.Product2Id__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>01tU0000001q3yt</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loopVarServices.Product2Id__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>01tU0000001q3yj</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loopVarServices.Product2Id__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>01tU0000001qB63</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loopVarServices.Product2Id__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>01tU0000001nev2</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>loopVarServices.Product2Id__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>01t4P00000AjvgO</stringValue>
                </rightValue>
            </conditions>
