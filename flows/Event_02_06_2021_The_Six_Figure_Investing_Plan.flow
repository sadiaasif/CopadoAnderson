    <decisions>
        <name>Offer_Type</name>
        <label>Offer Type</label>
        <locationX>414</locationX>
        <locationY>165</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>RAL_Success_Normal</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EventOfferAutomationText</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>The RAL Success - Education Package 2/06</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Campaign_IDs_0</targetReference>
            </connector>
            <label>RAL Success - Normal</label>
        </rules>
        <rules>
            <name>RAL_Success_Upgrade</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EventOfferAutomationText</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>The RAL Success - Education Package 2/06 Upgrade</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Campaign_IDs_1</targetReference>
            </connector>
            <label>RAL Success - Upgrade</label>
        </rules>
        <rules>
            <name>Normal_w_Bump</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>EventOfferAutomationText</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>The RAL Success - Education Package 2/06 Bump</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Campaign_IDs_0_0</targetReference>
            </connector>
            <label>Normal w/ Bump</label>
        </rules>
        <rules>
            <name>Upgrade_w_Bump</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>EventOfferAutomationText</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>The RAL Success - Education Package 2/06 Upgrade Bump</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Campaign_IDs_1_0</targetReference>
            </connector>
            <label>Upgrade w/ Bump</label>
        </rules>
    </decisions>
    <formulas>
        <name>OfferPurchaseTextFormula</name>
        <dataType>String</dataType>
        <expression>IF({!EventOfferAutomationText} = &quot;The RAL Success - Education Package 2/06&quot;, {!PurchasedDescriptionOffer1},
IF({!EventOfferAutomationText} = &quot;The RAL Success - Education Package 2/06 Bump&quot;, {!PurchasedDescriptionOffer2},
IF({!EventOfferAutomationText} = &quot;The RAL Success - Education Package 2/06 Upgrade Bump&quot;, {!PurchasedDescriptionOffer4},
IF({!EventOfferAutomationText} = &quot;The RAL Success - Education Package 2/06 Upgrade&quot;, {!PurchasedDescriptionOffer3},
&quot;Purchased package at The Six-Figure Investing Plan with Residential Living 02.06.2021&quot;))))</expression>
    </formulas>
    <formulas>
        <name>RALPricing</name>
        <dataType>Number</dataType>
        <expression>IF(CONTAINS({!EventOfferAutomationText}, &quot;Upgrade&quot;), 1500, 2997)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Wait</name>
        <dataType>DateTime</dataType>
        <expression>{!$Flow.CurrentDateTime} - (1/24)</expression>
    </formulas>
