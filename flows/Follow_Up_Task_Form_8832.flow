        <locationX>566</locationX>
        <locationY>51</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>EntityID</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>X8832_Email_Drip_Start__c</field>
            <value>
