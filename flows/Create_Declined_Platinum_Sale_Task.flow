    </formulas>
    <formulas>
        <name>NextBizDay</name>
        <dataType>Date</dataType>
        <expression>CASE(  
MOD( TODAY() - DATE( 1900, 1, 7 ), 7 ),
0, TODAY() + 1,
1, TODAY() + 1,
2, TODAY() + 1,
3, TODAY() + 1,
4, TODAY() + 1,
5, TODAY() + 3,
6, TODAY() + 2,

TODAY() + 1)</expression>
    </formulas>
