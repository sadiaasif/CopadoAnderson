    <description>This flow does the following
1- lookup the account object using lead company name
2- lookup the contact object using lead last name
3- populate the id of lead, account and contact to collection
4- pass the collection object to apex class to covert the lead and set the account and contact if found</description>
    <interviewLabel>LeadAutoConvertFlow {!$Flow.CurrentDateTime}</interviewLabel>
    <label>LeadAutoConvertFlow</label>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>accountLooup</name>
        <label>accountLooup</label>
        <locationX>25</locationX>
        <locationY>10</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>IsAccountCreated</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Name</field>
            <operator>Contains</operator>
            <value>
                <elementReference>LeadCompanyName</elementReference>
            </value>
        </filters>
        <filters>
            <field>PersonEmail</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>LeadEmail</elementReference>
            </value>
        </filters>
        <object>Account</object>
        <outputAssignments>
            <assignToReference>LeadAccount.Id</assignToReference>
            <field>Id</field>
        </outputAssignments>
    </recordLookups>
    <recordLookups>
        <name>contactLookup</name>
        <label>contactLookup</label>
        <locationX>34</locationX>
        <locationY>125</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>isContactCreated</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>LastName</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>LeadLastName</elementReference>
            </value>
        </filters>
        <filters>
            <field>Email</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>LeadEmail</elementReference>
            </value>
        </filters>
        <object>Contact</object>
        <outputAssignments>
            <assignToReference>LeadContact.Id</assignToReference>
            <field>Id</field>
        </outputAssignments>
    </recordLookups>
    <startElementReference>accountLooup</startElementReference>
    <status>Active</status>
