    <decisions>
        <name>Check_if_the_Client_Has_infinity_Subscription</name>
        <label>Check if the Client Has infinity Subscription</label>
        <locationX>361</locationX>
        <locationY>266</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Check_for_infinity_Subscription</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Infinity_Subscription_Sale</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Infinity_Subscription_Sale.Infinity_Memebership__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Membership_level__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Upgrade_the_Infinity_subscription</targetReference>
            </connector>
            <label>Check for infinity Subscription</label>
        </rules>
    </decisions>
    <decisions>
        <name>If_Event</name>
        <label>If Event</label>
        <locationX>693</locationX>
        <locationY>275</locationY>
        <defaultConnector>
            <targetReference>Subscription_Sale_update</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>EventofferAutomation_text_Present</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Event_Offer_automaton_text__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Upgrade_the_Infinity_subscription_0</targetReference>
            </connector>
            <label>EventofferAutomation text Present?</label>
        </rules>
    </decisions>
    <description>The Form assembly form will create a platform event, and this flow will check if the client has an infinity subscription and upgrade it based on the membership level</description>
    <formulas>
        <name>InfinityExpdate</name>
        <dataType>Date</dataType>
        <expression>DATE(
YEAR({!$Record.Sale_Date__c})+1,
MONTH({!$Record.Sale_Date__c}) ,
DAY({!$Record.Sale_Date__c})
)</expression>
    </formulas>
    <formulas>
        <name>SaleName</name>
        <dataType>String</dataType>
        <expression>If({!$Record.Membership_level__c}==&apos;360&apos;, &apos;Infinity Subscription - 360 Pro&apos;,{!Get_Infinity_Subscription_Sale.Name})</expression>
    </formulas>
    <interviewLabel>Subscription Upgrade platform Event - Sale Subscription upgrade {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Subscription Upgrade platform Event - Sale Subscription upgrade</label>
