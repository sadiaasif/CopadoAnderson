    <description>Assigns a follow up task to the Law Coordinator, CAM, or Anderson Advisors when an Online Q is created.</description>
    <formulas>
        <name>OneWeekLater</name>
        <dataType>Date</dataType>
        <expression>TODAY() + 7</expression>
    </formulas>
    <formulas>
        <name>OwnerFormula</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK({!ActiveLawCoordinatorId})), {!ActiveLawCoordinatorId},
IF(NOT(ISBLANK({!ActiveCAMId})), {!ActiveCAMId}, &quot;005U0000001DC1y&quot;))</expression>
    </formulas>
