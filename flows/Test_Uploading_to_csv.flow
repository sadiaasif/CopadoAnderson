    <formulas>
        <name>EmailConcatFormula</name>
        <dataType>String</dataType>
        <expression>If({!Get_Sale_Record.Account.E_Mail__c}=&apos;&apos;,{!Get_Sale_Record.Account.E_Mail__c}+&apos;,&apos;, &apos;&apos;)+
IF({!Get_Sale_Record.Account.Other_E_Mail__c}!=&apos;&apos; ,{!Get_Sale_Record.Account.Other_E_Mail__c}+&apos;,&apos;,&apos;&apos;)+
IF({!Get_Sale_Record.Account.Spouse_E_Mail__c}!=&apos;&apos; ,{!Get_Sale_Record.Account.Spouse_E_Mail__c}+&apos;,&apos;,&apos;&apos;)+
IF({!Get_Sale_Record.Account.Spouse_Other_E_Mail__c}!=&apos;&apos; ,{!Get_Sale_Record.Account.Spouse_Other_E_Mail__c}+&apos;,&apos;,&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>EventTime</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(LEFT(RIGHT(text( {!Get_Sale_Record.CreatedDate}),9),8))</expression>
    </formulas>
    <formulas>
        <name>PhonecontactFormula</name>
        <dataType>String</dataType>
        <expression>If({!Get_Sale_Record.Account.Phone_1__c}!=&apos;&apos;,{!Get_Sale_Record.Account.Phone_1__c}+&apos;,&apos;,&apos;&apos;)+
If({!Get_Sale_Record.Account.Phone_2__c}!=&apos;&apos;,{!Get_Sale_Record.Account.Phone_2__c}+&apos;,&apos;,&apos;&apos;)+
If({!Get_Sale_Record.Account.Phone_3__c}!=&apos;&apos;,{!Get_Sale_Record.Account.Phone_3__c}+&apos;,&apos;,&apos;&apos;)+
If({!Get_Sale_Record.Account.Phone_4__c}!=&apos;&apos;,{!Get_Sale_Record.Account.Phone_4__c}+&apos;,&apos;,&apos;&apos;)+
If({!Get_Sale_Record.Account.Phone_5__c}!=&apos;&apos;,{!Get_Sale_Record.Account.Phone_5__c}+&apos;,&apos;,&apos;&apos;)+
If({!Get_Sale_Record.Account.Phone_6__c}!=&apos;&apos;,{!Get_Sale_Record.Account.Phone_6__c}+&apos;,&apos;,&apos;&apos;)</expression>
    </formulas>
