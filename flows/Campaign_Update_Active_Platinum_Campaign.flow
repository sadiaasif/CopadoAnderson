    <decisions>
        <description>Determines whether to delete or create campaign members based on sale conditions.</description>
        <name>Sale_Conditions</name>
        <label>Sale Conditions</label>
        <locationX>204</locationX>
        <locationY>58</locationY>
        <defaultConnector>
            <targetReference>Find_Campaign_Members_0</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Inactive Platinum</defaultConnectorLabel>
        <rules>
            <name>Active_Platinum</name>
            <conditionLogic>(1 AND 2 AND 3) OR ((4 OR 5) AND 6 AND 1 AND 3)</conditionLogic>
            <conditions>
                <leftValueReference>SaleStage</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed Won</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>PaymentStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Recurring</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>TransactionTotal</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>NextTransactionDate</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>TODAY</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>PaymentStartDate</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>TODAY</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>PaymentStatus</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Recurring</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Find_Primary_Contact</targetReference>
            </connector>
            <label>Active Platinum</label>
        </rules>
    </decisions>
