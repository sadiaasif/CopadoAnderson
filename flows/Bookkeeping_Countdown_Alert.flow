    <decisions>
        <name>checkCountdown</name>
        <label>checkCountdown</label>
        <locationX>924</locationX>
        <locationY>351</locationY>
        <defaultConnector>
            <targetReference>loopOnBooks</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>countdownOverDue</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>loopOnBooks.Countdown_until_Completion__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>-1.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>NotifyBlanca</targetReference>
            </connector>
            <label>countdownOverDue</label>
        </rules>
