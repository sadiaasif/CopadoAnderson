<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <description>Works in conjuction with process builder to update the related entity of a living trust task every time it&apos;s edited.</description>
    <interviewLabel>Living Trust Related Entity Update {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Living Trust Related Entity Update</label>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>Find_Entity</name>
        <label>Find Entity</label>
        <locationX>282</locationX>
        <locationY>103</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>FindTrust</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Case__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>CaseConnect</elementReference>
            </value>
        </filters>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>012U0000000DE8w</stringValue>
            </value>
        </filters>
        <object>Entities__c</object>
        <outputAssignments>
            <assignToReference>EntityId</assignToReference>
            <field>Id</field>
        </outputAssignments>
    </recordLookups>
    <recordLookups>
        <name>Find_my_task_Case_Connect</name>
        <label>Find my task / Case Connect</label>
        <locationX>123</locationX>
        <locationY>103</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Find_Entity</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varTaskId</elementReference>
            </value>
        </filters>
        <object>Task</object>
        <outputAssignments>
            <assignToReference>CaseConnect</assignToReference>
            <field>Case_connect__c</field>
        </outputAssignments>
    </recordLookups>
    <recordLookups>
        <name>FindTrust</name>
        <label>Find Trust</label>
        <locationX>448</locationX>
        <locationY>104</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Update_Task</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Case__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>CaseConnect</elementReference>
            </value>
        </filters>
        <filters>
            <field>Name</field>
            <operator>Contains</operator>
            <value>
                <stringValue>Trust</stringValue>
            </value>
        </filters>
        <object>Trust__c</object>
        <outputAssignments>
            <assignToReference>varTrustId</assignToReference>
            <field>Id</field>
        </outputAssignments>
    </recordLookups>
    <recordUpdates>
        <name>Update_Task</name>
        <label>Update Task</label>
        <locationX>627</locationX>
        <locationY>103</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varTaskId</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Related_Entity__c</field>
            <value>
                <elementReference>EntityId</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Trust__c</field>
            <value>
                <elementReference>varTrustId</elementReference>
            </value>
        </inputAssignments>
        <object>Task</object>
    </recordUpdates>
    <startElementReference>Find_my_task_Case_Connect</startElementReference>
    <status>Active</status>
    <variables>
        <name>CaseConnect</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
