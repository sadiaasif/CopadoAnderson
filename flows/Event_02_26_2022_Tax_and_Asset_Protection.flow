    <formulas>
        <name>AmountBothBump</name>
        <dataType>Number</dataType>
        <expression>{!AmountLGZ} - 8000 - 2500</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>AmountLTBump</name>
        <dataType>Number</dataType>
        <expression>{!AmountLGZ} - 2500</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>AmountTitaniumBump</name>
        <dataType>Number</dataType>
        <expression>{!AmountLGZ} - 8000</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>OfferPurchaseTextFormula</name>
        <dataType>String</dataType>
        <expression>IF({!BumpLZS} = &quot;Titanium&quot;, {!PurchasedDescriptionOffer2}, 
IF({!BumpLZS} = &quot;LivingTrust&quot;,  {!PurchasedDescriptionOffer3},
IF({!BumpLZS} = &quot;Both&quot;,   {!PurchasedDescriptionOffer4}, {!PurchasedDescriptionOffer1})))</expression>
    </formulas>
    <formulas>
        <name>TransactionAmount</name>
        <dataType>Number</dataType>
        <expression>IF({!BumpLZS} = &quot;Titanium&quot;, {!AmountLGZ} + 8000, {!AmountLGZ})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>TXDateFormula</name>
        <dataType>DateTime</dataType>
        <expression>IF(NOT(ISBLANK({!TransactionDateLZS})), {!TransactionDateLZS}, {!$Flow.CurrentDateTime})</expression>
    </formulas>
    <formulas>
        <name>Wait</name>
        <dataType>DateTime</dataType>
        <expression>{!$Flow.CurrentDateTime} - (1/24)</expression>
    </formulas>
