<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes" access="global" controller="ProcessingConsoleCtrl" >
	 <aura:attribute name="recordId" type="Id"/>
    <aura:attribute name="ClientId" type="string"/>
    <aura:attribute name="ClientIDPresent" type="boolean"/>
    <aura:attribute name="BlueprintUrl" type="string"/>
    <aura:attribute name="baseUrl" type="String" default=""></aura:attribute>
    <aura:handler name="init" action="{!c.doInit}" value="{!this}" access="global" />
    <aura:if isTrue="{!v.ClientIDPresent}">
        
	 <iframe src="{!v.BlueprintUrl}" width="100%" height="500px;" frameBorder="0"/>
         <aura:set attribute="else">
             <div style="font-weight:bold;">
                 <center>The Task is not related to Any Client!</center>
             </div>
        </aura:set>
    </aura:if>
</aura:component>