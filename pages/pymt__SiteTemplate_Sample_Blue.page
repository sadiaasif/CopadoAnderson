 <style>

/* ------- Responsive CSS Nav Styling ----------- */
.nav_bar_shadow {
width:100%;
height: 10px;
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top, rgba(0,0,0,0.65) 0%, rgba(0,0,0,0) 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,0.65)), color-stop(100%,rgba(0,0,0,0))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%); /* IE10+ */
background: linear-gradient(to bottom, rgba(0,0,0,0.65) 0%,rgba(0,0,0,0) 100%); /* W3C */
/* filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#a6000000', endColorstr='#00000000',GradientType=0 ); */ /* IE6-8 */

}

.responsive_nav_bar {
   width:100%;
   background: steelblue; height: 50px;
}
.responsive_nav_bar > .inner {
    max-width:800px;
    margin-left:auto;
    margin-right:auto;

}

.css_menu {
}

/*Strip the ul of padding and list styling*/
.css_menu ul {
    z-index: 10000;
    list-style-type:none;
    margin:0;
    padding:0;
    position: absolute;
}

/*Create a horizontal list with spacing*/
.css_menu > ul > li {
    display:inline-block;
    float: left;
    margin-right: 0px;
}

/*Style for menu links*/
.css_menu li a {
    display:block;
    min-width:140px;
    height: 50px;
    text-align: center;
    line-height: 50px;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    color: #fff;
    background: transparent; /* steelblue; */
    text-decoration: none;
}

/*Hover state for top level links*/
#menu > li:hover > a {
    background: #2b506e; /* #19c589; */
}

/*Style for dropdown links*/
.css_menu li:hover ul a {
    background:  #f3f3f3;
    color: #2f3036;
    height: 40px;
    line-height: 40px;
}

/*Hover state for dropdown links*/
.css_menu li:hover ul a:hover {
    background: steelblue; /* #19c589; */
    color: #fff;
}

/*Hide dropdown links until they are needed*/
.css_menu li ul {
    display: none;
}

/*Make dropdown links vertical*/
#menu li ul li {
    display: block;
    float: none;
}

/*Prevent text wrapping*/
.css_menu li ul li a {
    width: auto;
    min-width: 100px;
    padding: 0 20px;
}

/*Display the dropdown on hover*/
.css_menu > ul > li > a:hover + .hidden, .css_menu .hidden:hover {
    display: block;
}

/*Style 'show menu' label button and hide it by default*/
.show-menu {
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    text-decoration: none;
    color: #fff;
    background: #2b506e; /* #19c589; */
    text-align: center;
    padding: 10px 0;
    display: none;
}

/*Hide checkbox*/
.css_menu input[type=checkbox]{
    display: none;
}

/*Show menu when invisible checkbox is checked*/
.css_menu input[type=checkbox]:checked ~ #menu{
    display: block;
}


/*Responsive Styles*/

@media screen and (max-width : 760px){
    /*Make dropdown links appear inline*/

.css_menu li a {
    background:  steelblue;
    }

    .css_menu ul {
        z-index: 10000;
        position: static;
        display: none;
    }
    /*Create vertical spacing*/
    .css_menu > ul > li {
        margin-bottom: 1px;
    }
    /*Make all menu links full width*/
    .css_menu ul li, li a {
        position:relative;
        z-index: 10001;
        width: 100%;
    }
    /*Display 'show menu' link*/
    .show-menu {
        display:block;
        margin-top: 0px; /* 11px; */
    }
}

/* ------- End of Responsive CSS Nav Styling -------- */

/* Fixed width layout (optional) version */
.pc_frame_top .inner {
   max-width:800px;
   margin-left:auto;
   margin-right:auto;

}
.pc_page_content_inner {
   max-width:800px;
   margin-left:auto;
   margin-right:auto;
}
</style>

