<apex:page standardController="H2S__HarvestProject__c" extensions="H2S.HarvestSyncProjectEntriesController" 
    action="{!sync}">
<apex:outputText value="{!H2S__HarvestProject__c.H2S__ProjectCode__c}{!H2S__HarvestProject__c.Name}" 
    rendered="false" />
<apex:form >
    <apex:pageBlock title="Harvest Sync">
        <p>An error occurred during sync: {!errorMessage}</p><br/>
        <apex:commandButton action="{!toProject}" value="Continue" />       
    </apex:pageBlock>
</apex:form>
</apex:page>