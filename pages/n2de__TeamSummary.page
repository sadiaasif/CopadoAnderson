                                                <apex:outputLabel value="{!$ObjectType.n2de__Team__c.Fields.n2de__Assign_to_field__c.Label}:" styleClass="team_label"/>
                                            </td>
                                            <td>
                                                <apex:outputText id="team_detail_assign_to_field" value="{!teamWrap.assignToField}"/>
                                            </td>
                                        </tr>
                                    </apex:outputPanel>
                                    <tr>
                                        <td height="5px;"></td>
                                    </tr>
                                    <tr>
