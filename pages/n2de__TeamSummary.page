    <link rel="stylesheet" href="{!URLFOR($Resource.DEResources, 'css/jquery.jui_dropdown.css')}" type="text/css" media="all"/>
    <c:CommonMask />
    <apex:form >
        <style>
    #pagecontainer {
        visibility: hidden;
        min-width: 1200px;
        overflow-x: auto;
        width: 100%;
    }
    .actions-dropdown * {
        font-family: Arial,Helvetica,sans-serif !important;
        font-size: 1em;
    }
    .actions-dropdown .ui-button-text{
        padding-top: 2px;
        padding-right: 22px;
        padding-bottom: 2px;
        padding-left: 5px; 
    }
    .actions-dropdown .action-menu {
        position: absolute;
        width: 140px;
        margin-top: 3px !important;
        z-index: 999999;
    }
    #team_menu_button {
        color: #333;
        margin: 1px;
        padding: 2px 3px;
        border: 1px solid #b5b5b5;
        border-bottom-color: #7f7f7f;
        background: #e8e8e9 url('/img/alohaSkin/btn_sprite.png') repeat-x right top;
        font-weight: bold;
        font-size: .9em;
        -moz-border-radius: 3px;
        -webkit-border-radius: 3px;
        border-radius: 3px;
    }
    h1.team_header{
        background-color: #c0c0c0;
        color: white;
        font-size: 10pt;
        margin-left: auto;
        margin-right: auto;
        display: block;
        clear: both;
        min-height: 25px;
        padding: 5px;
    }
    .container {
        float: left;
        margin: 10px;
        border: 1px solid gray;
        border-radius: 5px;
        display: block;
    }
    .short {
        overflow: auto;
        height: 270px;
    }
    .tall {
        overflow: auto;
        height: 680px;
    }
    .wide {
        width: 98%;
    }
    .detail-container{
        display:inline-block;
        vertical-align: top;
        padding: 0;
        overflow-x: auto;
    }
    .vertical-line {
        position: relative;
        top: 30px;
        height: 140px;
        border-left: 1px solid gray;
        width: 1%;
        margin-right: 10px;
    }
    .member-grid{
        border: 1px solid gray;
        background-color: #fafafa;
        float: left;
        margin: 5px;
        width: 170px;
        height: 55px;
        padding: 10px;
    }
    .member-grid div{
        float: left;
        height: 50px;
        overflow: auto;
    }
    .member-grid img{
        width: 35px;
        margin: 5px;
        margin-right:10px;
    }
    .member{
        margin-top: 5px;
        width: 100%;
    }
    .member td{
        backgroundcolor: grey;
        border-bottom: 1px solid lightgray;
    }
    .member img{
        width: 35px;
    }
    .member .status img{
        width: 25px;
        margin: 5px;
    }
    ::-webkit-scrollbar {
        width: 10px;
    }
    ::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 0 rgba(150, 150, 150, 1);
        background: rgb(180,180,180);
    }
    
    ::-webkit-scrollbar-thumb {
        border-radius: 10px;
        background: rgba(100, 100, 100, 1);
        -webkit-border-radius: 10px;
        -webkit-box-shadow: inset 0 0 6px rgba(50, 50, 50, 1);
    }
    
    ::-webkit-scrollbar-thumb:window-inactive {
        border-radius: 10px;
        background: rgba(100, 100, 100, 0.5);
        -webkit-border-radius: 10px;
        -webkit-box-shadow: inset 0 0 6px rgba(50, 50, 50, 0.5);
    }
    .label{
        margin-top: 5px;
    }
    .team_label{
        font-weight: bold;
        color: #4A4A56;
    }
    #clear{
        clear: both;
    }
    .disabled{
        background-color: #ededed !important; 
        border: 1px solid LightGray !important;
    }
    .disabled img{
        opacity : 0.4;
    }
    .out_office{
        background-color: #ededed; 
        background-image: url({!URLFOR($Resource.n2de__DEResources, 'images/outofoffice32.png')});
        background-position: right bottom;
        background-repeat: no-repeat;
        opacity: 0.7;
    }
    .out_office img{
        opacity : 0.4;
    }
    .offline{
        background-color: #ededed; 
        background-image: url({!URLFOR($Resource.n2de__DEResources, 'images/notavailable32.png')});
        background-position: right bottom;
        background-repeat: no-repeat;
        opacity: 0.7;
        background-size: 20px;
    }
    .offline img{
        opacity : 0.7;
    }
    .list_icon{
        width: 25px !important;
        padding: 1px !important;
    }
    .de_help_icon{
        position: absolute;
        right: 10px;
        top: 10px;
    }
    .edit_hours:hover {
        cursor: pointer;
    }
    .dist-action-link:hover {
        cursor: pointer;
    }
    .tab-area { 
        padding: 0px; 
        background: none; 
        border-width: 0px; 
    }
    .tab-area .ui-widget-header {
        background: #c0c0c0;
        border-bottom-right-radius: 0;
        border-bottom-left-radius: 0;
    }
    .tab-area-dist * {
        color: #222;
    }
    .tab-area * {
        font-family: Arial,Helvetica,sans-serif !important;
    }
    .tab-area a{ 
        outline: 0;
    } 
    .dist_list{
        min-height: 580px;
        padding: 0;
        margin: 0;
        width: 100%;
    } 
    .distributor:hover{
        background-color: beige;
    }
    .distributor{
        border: 1px solid #A6A6A6;
        border-radius: 4px;
        background-color: #fafafa;
        margin: 5px;
        padding: 10px;
        height: 55px;
        list-style: none;
        padding-left: 0;
        margin-left: 5px;
    }
    .distributor .dist-item-container {
        vertical-align: middle;
        height: 55px;
        display: table-cell;
    }
    .sortable .distributor{
        cursor:move;
    }
    .distributor img{
        width: 35px;
        margin: 5px;
        margin-left: 10px;
        float: left;
    }
    .distributor-lbl {
        margin: 15px;
        float: left;
        width: 200px;
    }
    .distributor div.img{
        width: 50px;
    }
    .distributor .details{
        clear: both;
        color: gray;
        width: 100%;
    }
    .sub-dist-container {
        display: inline-block;
        width: 47%;
        height: 630px;
        overflow-x: hidden;
        overflow-y: auto;
    }
    .vertical-line-tall {
        position: relative;
        height: 630px;
        border-left: 1px solid gray;
        width: 1%;
        margin-right: 10px;
        margin-left: 10px;
    }
    .advanced-dist-container {
        display: inline-block;
        width: 98%;
        height: 630px;
        overflow: auto;
    }
    #add_adv_button {
      color: #333;
      margin: 1px;
      padding: 2px 3px;
      border: 1px solid #b5b5b5;
      border-bottom-color: #7f7f7f;
      background: #e8e8e9 url('/img/alohaSkin/btn_sprite.png') repeat-x right top;
      font-weight: bold;
      font-size: .9em;
      -moz-border-radius: 3px;
      -webkit-border-radius: 3px;
      border-radius: 3px;
    }
    .new-help {
        display: table; 
        margin:0 auto;
        font-size: 15px;    
    }
    .new-help a {
        color: #56aa1c;
    }
    .new-help a:hover {
        color: #bbdda4;
    }
    .tag-detail {
        font-style: italic;
    }
    .dist-detail-img {
        width: 20px !important;
        opacity: 0.67;
    }

        </style>
        <script>
var teamId = '{!JSENCODE(teamWrap.Id)}';
$(document).ready(function() {
    setSizes();
    $('.tab-area').tabs();
    var activeTabId = $('.tab-area-dist .ui-tabs-active a').attr('id');
    showAddButtons(activeTabId);
    $( ".sortable" ).sortable({
        update: function( event, ui ) {
            onChangeDistributorList(ui);
        }
    });
    $('[class$="_shiftEdit"]').change(function() {
        console.log('changing');
        var memberId = this.getAttribute('userid');
        updateShiftHours(memberId);
    });
    $('.online_toggle').iphoneStyle({
        onChange: function(elem, isOn) {
           var userId = $(elem).parent().parent().parent().attr('user_id');
           toggleOnline(userId, isOn);
        }
    });
     $("#team_actions").jui_dropdown({
        launcher_id: 'team_menu_button',
        containerClass: 'actions-dropdown',
        launcher_container_id: 'tm_container',
        menu_id: 'team_action_menu',
        launcherClass: 'btn',
        menuClass: 'action-menu',
        my_position: 'right top',
        at_position: 'right bottom'
    });
    <apex:repeat value="{!distributorList}" var="d">
        $('#dist_actions_{!d.Id}').jui_dropdown({
            launcher_id: 'dist_menu_button_{!d.Id}',
            containerClass: 'actions-dropdown',
            launcher_container_id: 'dm_container_{!d.Id}',
            menu_id: 'dist_action_menu_{!d.Id}',
            launcherClass: 'btn',
            menuClass: 'action-menu',
            my_position: 'right top',
            at_position: 'right bottom'
        });
    </apex:repeat>
    
    if ('{!teamWrap.isDistributeToQueue}' == 'false') {
        <apex:repeat value="{!pullDistributorList}" var="d">
            $('#dist_actions_{!d.Id}').jui_dropdown({
                launcher_id: 'dist_menu_button_{!d.Id}',
                containerClass: 'actions-dropdown',
                launcher_container_id: 'dm_container_{!d.Id}',
                menu_id: 'dist_action_menu_{!d.Id}',
                launcherClass: 'btn',
                menuClass: 'action-menu',
                my_position: 'right top',
                at_position: 'right bottom'
            });
        </apex:repeat>
        if ('{!TriggersEnabled}' == 'true') {
            <apex:repeat value="{!triggerList}" var="d">
                $('#dist_actions_{!d.Id}').jui_dropdown({
                    launcher_id: 'dist_menu_button_{!d.Id}',
                    containerClass: 'actions-dropdown',
                    launcher_container_id: 'dm_container_{!d.Id}',
                    menu_id: 'dist_action_menu_{!d.Id}',
                    launcherClass: 'btn',
                    menuClass: 'action-menu',
                    my_position: 'right top',
                    at_position: 'right bottom'
                });
            </apex:repeat>
        }
        $('#advanced_add_container').jui_dropdown({
            launcher_id: 'add_adv_button',
            containerClass: 'actions-dropdown',
            launcher_container_id: 'adv_dist_add',
            menu_id: 'advanced_add_list',
            launcherClass: 'btn',
            menuClass: 'action-menu',
            my_position: 'right top',
            at_position: 'right bottom'
        });
    }
    $('#member_action_container').jui_dropdown({
        launcher_id: 'tm_action_button',
        containerClass: 'actions-dropdown',
        launcher_container_id: 'team_member_actions',
        menu_id: 'tm_action_menu',
        launcherClass: 'btn',
        menuClass: 'action-menu',
        my_position: 'right top',
        at_position: 'right bottom'
    });
    document.getElementById('pagecontainer').style.visibility = "visible";
});
$(window).resize(function() {
    setSizes();
});
function setSizes(){
    $(".distributor-container").css("min-height", $(".distributor-container").height());
}
function onChangeDistributorList(ui){
    saveOrder(ui);
}
function saveOrder(ui){
    var listElementId = ui.item[0].id;
    showMask();
    var distributorIdArray = new Array();
    $("#" + listElementId).parent().find("li.distributor").each(function(){
        var distributorId = $(this).attr('id');
        if(distributorId != undefined){
            distributorIdArray.push(distributorId);
        }
    });
    Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.TeamController.saveOrder}', distributorIdArray, teamId, 
        function(result, event){
            hideMask();
            if (event.type === 'exception'){
                alert(event.message);
            }else{
                //document.location.href = '/apex/TeamSummary?id=' + teamId;
            }
    });
}
function toggleOnline(userId, isOnline){
    showMask();
    Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.TeamController.toggleOnline}', userId, isOnline, 
        function(result, event){
            hideMask();
            if (event.type === 'exception'){
                alert(event.message);
            }else{
                //document.location.href = '/apex/TeamSummary?id=' + teamId;
            }
            hideMask();
    });
}

function changeMemberShiftHours(memberId) {
    $('#' + memberId + '_edit_tm_hours').hide();
    $('#' + memberId + '_cancel_tm_hours').show();
    $('.' + memberId + '_distHoursDisplay').hide();
    $('.' + memberId + '_shiftEdit').show();
    $('.' + memberId + '_shiftEdit').attr('userid', memberId);
    var sel = $('.' + memberId + '_shiftEdit').val();
    $('.' + memberId + '_shiftEdit').attr('spid', sel);
}
function cancelMemberShiftHours(memberId) {
    $('#' + memberId + '_edit_tm_hours').show();
    $('#' + memberId + '_cancel_tm_hours').hide();
    $('.' + memberId + '_distHoursDisplay').show();
    $('.' + memberId + '_shiftEdit').hide();
}

function updateShiftHours(memberId) {
    showMask();
    var shiftId = $('.' + memberId + '_shiftEdit').val();
    Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.TeamController.changeTeamMemberShiftHours}', memberId, shiftId, 
        function(result, event){
            if (event.type === 'exception'){
                alert(event.message);
                var oldVal = $('.' + memberId + '_shiftEdit').attr('spid');
                $('.' + memberId + '_shiftEdit').val(oldVal);
            } else {
                location.reload();
            }
    });
}

    function showTeamActions() {
        $('#team_actions').show();
        $('#tracking_actions').hide();
        
    }

    function showTrackingActions() {
        $('#tracking_actions').show();
        $('#team_actions').hide();
    }


        </script>
        <script defer="defer" src="{!URLFOR($Resource.DEResources, 'scripts/ga.js')}"></script>

        <apex:sectionHeader title="Distribution Team" subtitle="{!teamWrap.Name}"/>
