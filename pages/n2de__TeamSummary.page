                                        </tr>
                                        <tr>
                                            <td></td>
                                            <td><span>Cap type </span>&nbsp;
                                                <apex:outputText id="team_detail_cap_type" styleClass="tag-detail" value="{!teamWrap.TeamLevelCapLabel}"/>
                                            </td>
                                        </tr>
                                    </apex:outputPanel>
