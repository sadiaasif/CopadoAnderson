                                        <td>
                                            <apex:outputLabel value="{!$ObjectType.n2de__Distribution_hours__c.Fields.n2de__Shift_name__c.Label}:" styleClass="team_label"/>
                                        </td>
                                        <td>
                                            <apex:outputText id="team_detail_shift_hours" value="{!IF(DistributionHours.n2de__Is_shift_hours__c, DistributionHours.n2de__Shift_name__c, '-- Custom hours --')}"/>&nbsp;&nbsp;&nbsp;<apex:outputLink id="team_detail_shift_hours_link" value="/apex/{!NamespacePrefix}ShiftHours?returnTo=TeamSummary&obj={!teamWrap.team.Id}">Manage shift hours</apex:outputLink>
