        <active>false</active>
        <description>Prevents selection of invalid scheduled processing actions</description>
        <errorConditionFormula>ISPICKVAL( pymt__Scheduled_Processing_Action__c ,&apos;Invalid Action&apos;)</errorConditionFormula>
        <errorDisplayField>pymt__Scheduled_Processing_Action__c</errorDisplayField>
        <errorMessage>The selected scheduled processing action is not supported in this version of PaymentConnect.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pymt__Scheduled_Payment_Required_Fields</fullName>
        <active>true</active>
        <description>Flags required fields for a Scheduled Payment record type</description>
        <errorConditionFormula>AND( ISPICKVAL(pymt__Profile_Type__c,  &apos;Scheduled Payment&apos;),  OR(ISNULL( pymt__Amount__c), ISBLANK(  TEXT(pymt__Period__c) ), ISBLANK(  TEXT(pymt__Frequency__c) ), ISNULL(  pymt__Subscription_Start_Date__c ), ISBLANK(  TEXT(pymt__Currency_ISO_Code__c) ) ) )</errorConditionFormula>
        <errorMessage>Required fields: Amount, Period, Frequency, Subscription Start Date, &amp; Currency ISO Code</errorMessage>
    </validationRules>
