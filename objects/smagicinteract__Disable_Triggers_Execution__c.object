<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>This setting allows administrators to disable certain triggers on SMS Magic managed package.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>smagicinteract__Disable_Incoming_Lookup__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Check this field to stop lookup and actions happening on incoming messages from managed package code.</description>
        <externalId>false</externalId>
        <label>Disable Incoming Lookup</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>smagicinteract__Disable_Lead_Conversion__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Check this field to stop migration of messages and conversations on lead migration.</description>
        <externalId>false</externalId>
        <label>Disable Lead Conversion</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>smagicinteract__Disable_Mail_Sending__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Check this field to disable managed package code to send email notifications on incoming messages.</description>
        <externalId>false</externalId>
        <label>Disable Mail Sending</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>smagicinteract__Disable_Salesforce_Task__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Check this field to disable sending of automated messages on salesforce task creation</description>
        <externalId>false</externalId>
        <label>Disable Salesforce Task</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Disable Triggers Execution</label>
    <visibility>Public</visibility>
</CustomObject>
