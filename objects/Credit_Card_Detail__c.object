    <validationRules>
        <fullName>AMEX_Card_Number_Length</fullName>
        <active>false</active>
        <description>AMEX must be a length of 15 digits.</description>
        <errorConditionFormula>ISPICKVAL( Card_Type__c , &apos;AMEX&apos;) &amp;&amp;
LEN( Card_Number__c ) &lt;&gt; 15</errorConditionFormula>
        <errorDisplayField>Card_Number__c</errorDisplayField>
        <errorMessage>AMEX cards must have a length of 15 digits.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AMEX_Security_Code</fullName>
        <active>false</active>
        <description>AMEX Security Code must contain four digits.</description>
        <errorConditionFormula>ISPICKVAL( Card_Type__c , &apos;AMEX&apos;) &amp;&amp;
 LEN( Card_Security__c ) &lt;&gt; 4 &amp;&amp;
NOT(LEN(Card_Security__c ) = 0)</errorConditionFormula>
        <errorDisplayField>Card_Security__c</errorDisplayField>
        <errorMessage>AMEX security code must contain four digits.</errorMessage>
    </validationRules>
