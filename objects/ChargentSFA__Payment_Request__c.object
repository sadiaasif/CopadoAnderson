    <validationRules>
        <fullName>ChargentSFA__Block_inactive_PR_transaction_types</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR( ISCHANGED(ChargentSFA__Payment_Request_Transaction_Type__c) , ISNEW() ), 
NOT( 
OR( 
IF( 
INCLUDES( 
ChargentSFA__Opportunity__r.ChargentSFA__Gateway__r.ChargentBase__Available_PR_Transaction_Types__c,&quot;Authorize Full Amount&quot; ), 
IF( 
ISPICKVAL(ChargentSFA__Payment_Request_Transaction_Type__c,&quot;Authorize Full Amount&quot; ), true, false 
), 
false 
), 
IF( 
INCLUDES( 
ChargentSFA__Opportunity__r.ChargentSFA__Gateway__r.ChargentBase__Available_PR_Transaction_Types__c,&quot;Charge Full Amount&quot; ), 
IF( 
ISPICKVAL(ChargentSFA__Payment_Request_Transaction_Type__c,&quot;Charge Full Amount&quot; ), true, IF(ISPICKVAL(ChargentSFA__Payment_Request_Transaction_Type__c,&quot;&quot; ),true, false) 
), 
false 
), 
IF( 
INCLUDES( 
ChargentSFA__Opportunity__r.ChargentSFA__Gateway__r.ChargentBase__Available_PR_Transaction_Types__c,&quot;Authorize Minimum Amount&quot; ), 
IF( 
ISPICKVAL(ChargentSFA__Payment_Request_Transaction_Type__c,&quot;Authorize Minimum Amount&quot; ), true, false 
), 
false 
), 
IF( 
ISBLANK( 
ChargentSFA__Opportunity__r.ChargentSFA__Gateway__r.ChargentBase__Available_PR_Transaction_Types__c), 
IF( 
ISPICKVAL(ChargentSFA__Payment_Request_Transaction_Type__c,&quot;Charge Full Amount&quot; ), true, IF(ISPICKVAL(ChargentSFA__Payment_Request_Transaction_Type__c,&quot;&quot; ), true, false) 
), 
false 
) 
) 
) 
)</errorConditionFormula>
        <errorDisplayField>ChargentSFA__Payment_Request_Transaction_Type__c</errorDisplayField>
        <errorMessage>Transaction Type is not allowed.</errorMessage>
    </validationRules>
