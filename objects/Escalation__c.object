    <fields>
        <fullName>Days_in_Escalation__c</fullName>
        <externalId>false</externalId>
        <formula>(
(5 * ( FLOOR( ( IF(ISBLANK(Escalation_Cleared__c),TODAY(),Escalation_Cleared__c) - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( IF(ISBLANK(Escalation_Cleared__c),TODAY(),Escalation_Cleared__c) - DATE( 1900, 1, 8), 7 ) ) )
-
(5 * ( FLOOR( ( DATEVALUE(Created_DateTime__c) - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( DATEVALUE(Created_DateTime__c) - DATE( 1900, 1, 8), 7 ) ) )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Days in Escalation</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
