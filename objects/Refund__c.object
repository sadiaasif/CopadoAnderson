If((Owner:User.FirstName + SUBSTITUTE( Owner:User.LastName , &quot; (Community Test)&quot;, &quot;&quot;) )= ($User.FirstName + $User.LastName ),True,False),If((Owner:User.FirstName+ Owner:User.LastName)=($User.FirstName + SUBSTITUTE( $User.LastName , &quot; (Community Test)&quot;, &quot;&quot;) ),True,false)) ,

