    <validationRules>
        <fullName>ChargentOrders__Block_inactive_PR_transaction_types</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR( ISCHANGED(ChargentOrders__Payment_Request_Transaction_Type__c) , ISNEW() ),
              NOT( 
                    OR( 
                          IF( 
                              INCLUDES( 
                                  ChargentOrders__ChargentOrder__r.ChargentOrders__Gateway__r.ChargentBase__Available_PR_Transaction_Types__c,&quot;Authorize Full Amount&quot; ), 
                                      IF( 
                                          ISPICKVAL(ChargentOrders__Payment_Request_Transaction_Type__c,&quot;Authorize Full Amount&quot; ), true, false 
                                      ), 
                              false 
                          ), 
                          IF( 
                              INCLUDES( 
                                  ChargentOrders__ChargentOrder__r.ChargentOrders__Gateway__r.ChargentBase__Available_PR_Transaction_Types__c,&quot;Charge Full Amount&quot; ), 
                                  IF( 
                                  ISPICKVAL(ChargentOrders__Payment_Request_Transaction_Type__c,&quot;Charge Full Amount&quot; ), true, IF(ISPICKVAL(ChargentOrders__Payment_Request_Transaction_Type__c,&quot;&quot; ),true, false) 
                              ), 
                          false 
                          ), 
                          IF( 
                              INCLUDES( 
                              ChargentOrders__ChargentOrder__r.ChargentOrders__Gateway__r.ChargentBase__Available_PR_Transaction_Types__c,&quot;Authorize Minimum Amount&quot; ), 
                              IF( 
                              ISPICKVAL(ChargentOrders__Payment_Request_Transaction_Type__c,&quot;Authorize Minimum Amount&quot; ), true, false 
                          ), 
                          false 
                          ),
                           IF( 
                              ISBLANK( 
                                  ChargentOrders__ChargentOrder__r.ChargentOrders__Gateway__r.ChargentBase__Available_PR_Transaction_Types__c), 
                                  IF( 
                                        ISPICKVAL(ChargentOrders__Payment_Request_Transaction_Type__c,&quot;Charge Full Amount&quot; ), true, IF(ISPICKVAL(ChargentOrders__Payment_Request_Transaction_Type__c,&quot;&quot; ), true, false)
                                  ), 
                          false 
                          )
                  ) 
          ) 
  )</errorConditionFormula>
        <errorDisplayField>ChargentOrders__Payment_Request_Transaction_Type__c</errorDisplayField>
        <errorMessage>Transaction Type is not allowed.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
