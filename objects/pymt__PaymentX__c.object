subtotal += item.getFloat(&quot;pymt__Total__c&quot;);

}
subtotal -= parseFloat(&quot;{!NULLVALUE(TEXT(pymt__PaymentX__c.pymt__Discount__c), &apos;0.0&apos;) }&quot;)
var total = subtotal + parseFloat(&quot;{!NULLVALUE(TEXT(pymt__PaymentX__c.pymt__Tax__c), &apos;0.0&apos;) }&quot;) + parseFloat(&quot;{!NULLVALUE( pymt__PaymentX__c.pymt__Shipping__c, &apos;0&apos;) }&quot;);

