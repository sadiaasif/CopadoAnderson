    <webLinks>
        <fullName>evt__Reset_Invitation</fullName>
        <availability>online</availability>
        <description>Changes eligible attendee records back to &quot;Invited&quot; status.</description>
        <displayType>link</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reset Invitation</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 

// Check status value: If Registered, don&apos;t change
var status = &quot;{!evt__Attendee__c.evt__Invitation_Status__c}&quot;;

if (status == &apos;Registered&apos;) {
   alert(&apos;Attendee has already registered.  Cannot reset to &quot;Invited&quot; status.&apos;);
} else {
   // See if the attendee has a completed payment 
    var hasPmt = &quot;{!BLANKVALUE(evt__Attendee__c.evt__Payment__c,&apos;&apos;)}&quot; !== &quot;&quot;;
    // query
    var results;
    var pmt;
    var status;
    var pmtValue;
    if (hasPmt) {
        results = sforce.connection.query(&quot;Select Id, pymt__Status__c, pymt__Is_Scheduled__c, pymt__Amount__c From pymt__PaymentX__c where id = &apos;{!evt__Attendee__c.evt__PaymentId__c}&apos;&quot;);
        pmt = results.getArray(&apos;records&apos;)[0];
        status = pmt.pymt__Status__c;
        pmtValue = pmt.pymt__Amount__c;
    }
    if (hasPmt &amp;&amp; status == &apos;Completed&apos;) {
       alert(&apos;Attendee is attached to a completed payment (in the amount of &apos;+pmtValue+&apos;) indicating registration may have been completed. \n\nYou may want to change this attendee to &quot;Registered&quot; - or detach the payment before resetting this record\&apos;s invitation status to &quot;Invited&quot;.&apos;);
    } else if (hasPmt &amp;&amp; pmt.getBoolean(&quot;pymt__Is_Scheduled__c&quot;)) {
       alert(&apos;Attendee is attached to a scheduled payment (in the amount of &apos;+pmtValue+&apos;) indicating payment for registration is expected. \n\nVoid or detach the scheduled payment before resetting this record\&apos;s invitation status to &quot;Invited&quot;.&apos;);

    } else {
      

      // If attendee has an &quot;In Process&quot; payment, warn before reset.
      var cont = true;
      if (hasPmt &amp;&amp; status == &apos;In Process&apos;) {
          cont = confirm(&apos;Attendee is attached to an &quot;In Process&quot; payment indicating that a registration payment may be in process.  Do you still want to reset this record to invitation status &quot;Invited&quot; and separate it from the payment?&apos;);
      }
      if (cont == true) {

     
         // Reset attendee record
         var newRecords = []; 
         var item = new sforce.SObject(&quot;evt__Attendee__c&quot;); 
         item.id = &apos;{!evt__Attendee__c.Id}&apos;; 
         item.evt__Invitation_Status__c = &apos;Invited&apos;;
         item.evt__Payment__c = null; 
         newRecords.push(item); 
         var result = sforce.connection.update(newRecords); 
         if (result[0].getBoolean(&quot;success&quot;)) {
               window.location.reload();
         } else {
            alert(&apos;Failed to update attendee record: &apos;+ result[0]); 
         }
      } 
   }
}</url>
    </webLinks>
</CustomObject>
