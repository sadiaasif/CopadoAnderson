    <webLinks>
        <fullName>evt__Email_Event_Pass</fullName>
        <availability>online</availability>
        <description>Sends an event pass to the attendee.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Email Event Pass</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/connection.js&quot;)}

var emailTemplateDeveloperName = &quot;Event_Pass_PDF_Attendee&quot;; // This template name can be changed to any custom template you choose. Be sure to build your custom template around the evt__Attendee__c object.

var query = &quot;Select id from EmailTemplate where isActive = true and DeveloperName = &apos;&quot;+emailTemplateDeveloperName+&quot;&apos; &quot;;
var result = sforce.connection.query(query);
var templates = result.getArray(&quot;records&quot;);

if (templates.length == 0) {
alert(&quot;Sorry, could not find an active email template named \&quot;&quot;+emailTemplateDeveloperName+&quot;\&quot;. Please contact your Salesforce administrator for help in setting up this template.&quot;);

} else {
   var abort = false;
   if (&quot;{!evt__Attendee__c.evt__Invitation_Status__c}&quot; !== &quot;Registered&quot; ) {
       abort = !confirm(&quot;This attendee is not yet registered.  Send event pass anyway?&quot;);
   }
   var templateId = templates[0].Id;
   if (abort !== true) {
window.location = &quot;/_ui/core/email/author/EmailAuthor?p3_lkid={!evt__Attendee__c.Id}&amp;p2_lkid={!evt__Attendee__c.evt__ContactId__c}&amp;template_id=&quot;+templateId+&quot;&amp;cancelURL=/{!evt__Attendee__c.Id}&amp;retURL=/{!evt__Attendee__c.Id}&amp;new_template=1&quot;;
   }
}</url>
    </webLinks>
    <webLinks>
        <fullName>evt__Email_Invitation</fullName>
        <availability>online</availability>
        <description>Sends an event invitation to the attendee.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Email Invitation</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/connection.js&quot;)}

var emailTemplateDeveloperName = &quot;Linvio_Events_Invitation_Attendee&quot;; // This template name can be changed to any custom template you choose. Be sure to build your custom template around the evt__Attendee__c object.

var query = &quot;Select id from EmailTemplate where isActive = true and DeveloperName = &apos;&quot;+emailTemplateDeveloperName+&quot;&apos; &quot;;
var result = sforce.connection.query(query);
var templates = result.getArray(&quot;records&quot;);

if (templates.length == 0) {
alert(&quot;Sorry, could not find an active email template named \&quot;&quot;+emailTemplateDeveloperName+&quot;\&quot;. Please contact your Salesforce administrator for help in setting up this template.&quot;);

} else if (&quot;{!evt__Attendee__c.evt__Invitation_Status__c}&quot; == &quot;Registered&quot; ) {
alert(&quot;This attendee has already registered. &quot;);

} else {
var templateId = templates[0].Id;
window.location = &quot;/_ui/core/email/author/EmailAuthor?p3_lkid={!evt__Attendee__c.Id}&amp;p2_lkid={!evt__Attendee__c.evt__ContactId__c}&amp;template_id=&quot;+templateId+&quot;&amp;cancelURL=/{!evt__Attendee__c.Id}&amp;retURL=/{!evt__Attendee__c.Id}&amp;new_template=1&quot;;

}</url>
