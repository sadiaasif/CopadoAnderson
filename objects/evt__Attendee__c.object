    <webLinks>
        <fullName>evt__Mark_Attended</fullName>
        <availability>online</availability>
        <description>Marks selected attendees records as &quot;Attended&quot;</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mark Attended</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}

var records = {!GETRECORDIDS( $ObjectType.evt__Attendee__c )};

if (records[0] == null) {
  alert(&quot;Please select at least one record.&quot;) }
else {

	var newRecords = [];
	for (var n in records) {
	  var item = new sforce.SObject(&quot;evt__Attendee__c&quot;);
	  item.id = records[n];
	  item.evt__Attended__c = true;
	  newRecords.push(item);
	}
	var result = sforce.connection.update(newRecords);
	if (result &amp;&amp; result.length){
	  var numFailed = 0;
	  var numSucceeded = 0;
	  for (var i = 0; i &lt; result.length; i++){
	    var res = result[i];
	    if (res &amp;&amp; res.success == &apos;true&apos;){
	      numSucceeded++;
	    } else {
	      var es = res.getArray(&quot;errors&quot;);
	      if (es.length &gt; 0) {
	        errors.push(es[0].message);
	      }
	      numFailed++;
	   }
	 }
	if (numFailed &gt; 0){
	  alert(&quot;Failed: &quot; + numFailed + &quot;\nSucceeded: &quot; + numSucceeded + &quot; \n Due to: &quot; +   errors.join(&quot;\n&quot;));
	} else {
	  alert(&quot;Number of records updated: &quot; + numSucceeded);
	}
  }
  window.location.reload();
}</url>
    </webLinks>
