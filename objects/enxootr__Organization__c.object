    <validationRules>
        <fullName>enxootr__ORG_VR01_TestClassNamingScheme</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  NOT(ISBLANK(enxootr__Test_Class_Naming_Scheme__c )),
		NOT(REGEX(enxootr__Test_Class_Naming_Scheme__c,&quot;^\\w*\\*\\w*$&quot;))
)</errorConditionFormula>
        <errorDisplayField>enxootr__Test_Class_Naming_Scheme__c</errorDisplayField>
        <errorMessage>Test class naming scheme has to consist only of characters valid for Apex class name AND contain exactly one wildcard character *. [ORG_VR01]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>enxootr__ORG_VR02_UniqueIdMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				ISNEW(),
				ISBLANK(enxootr__Unique_Id__c)  
				)</errorConditionFormula>
        <errorDisplayField>enxootr__Unique_Id__c</errorDisplayField>
        <errorMessage>Unique Id is mandatory. [ORG_VR02]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>enxootr__ORG_VR03_CredentialsMandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				OR(
				  ISNEW(),
						ISCHANGED(enxootr__Login_Url__c),
						ISCHANGED(enxootr__Local_org__c),
						ISCHANGED(enxootr__Username__c),
						ISCHANGED(enxootr__Password_Encrypted__c)
				),
				NOT(enxootr__Local_org__c),
				OR(
						ISBLANK(enxootr__Login_Url__c),
				  ISBLANK(enxootr__Username__c),
						ISBLANK(enxootr__Password_Encrypted__c)
				)
)</errorConditionFormula>
        <errorMessage>When Local Org checkbox is deselected, Login URL, Username and Password fields are mandatory. [ORG_VR03]</errorMessage>
    </validationRules>
